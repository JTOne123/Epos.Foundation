{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [ "Epos.Utilities/**/*.cs", "Epos.CmdLine/**/*.cs" ],
          "exclude": [ "**/obj/**", "**/bin/**" ]
        }
      ],
      "dest": "Api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "Api/**.yml" ] },
      { "files": [ "toc.yml", "getting-started.md", "Articles/**.md", "Articles/**/toc.yml" ] }
    ],
    "resource": [
      { "files": [ "Images/**" ] }
    ],
    "overwrite": [
      { "files": [ "Overwrite/**.md" ] }
    ],
    "globalMetadata": {
      "_appTitle": "Epos.Foundation Docs",
      "_enableSearch": false
    },
    "dest": "bin/Website",
    "fileMetadataFiles": [],
    "template": [ "Templates/Epos" ],
    "postProcessors": [],
    "noLangKeyword": false,
    "xref": "msdn.4.5.2.zip"
  }
}
